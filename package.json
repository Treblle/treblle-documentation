        "@pandacss/dev": "^1.8.1",
        "@treblle/eslint-config": "^1.1.0",
        "@treblle/prettier-config": "^1.0.2",
        "@treblle/typescript-config": "^1.0.1",
        "@types/node": "^20.17.14",
        "@types/react": "^19.2.7",
